                datePickerSet($("#start_date"), $("#end_date"), true);
            });
            $(".year").click(function () {
                let st = new Date($("#start_date").val());
                st.setFullYear(st.getFullYear() - 1);
                $("#start_date").val(st);
                datePickerSet($("#start_date"), $("#end_date"), true);
            });

        });
