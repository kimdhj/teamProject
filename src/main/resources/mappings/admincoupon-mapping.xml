<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AdminCoupon">
	
	<resultMap type="couponVO" id="adminCouponResult">
		<result property="user_id" column="USER_ID" />
		<result property="coupon_seq" column="USER_COUPON_SEQ" />
		<result property="coupon_code" column="USER_COUPON_CODE" />
		<result property="coupon_date" column="USER_COUPON_DATE" />
		<result property="coupon_name" column="USER_COUPON_NAME" />
		<result property="coupon_effect" column="USER_COUPON_EFFECT" />
	</resultMap>
	
	<select id="getUserCouponList" parameterType="string" resultMap="adminCouponResult">
		SELECT * FROM user_coupon WHERE user_id = #{user_id}
	</select>	
	
	
</mapper>